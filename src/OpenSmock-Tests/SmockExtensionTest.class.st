Class {
	#name : #SmockExtensionTest,
	#superclass : #TestCase,
	#category : #'OpenSmock-Tests-Symbology'
}

{ #category : #tests }
SmockExtensionTest >> testCombineSubCollections [

	| vals |
	vals := {
		        #( 0 12 ).
		        #( 25 50 80 ).
		        #( 100 200 300 ).
		        #( 66 22 ) } combineSubCollections.
	self assert: vals size = 36.
	self assert: (vals allSatisfy: [ :e | e size = 4 ]).
	self assert: (STON fromString:
			 'OrderedCollection[[0,25,100,66],[0,25,100,22],[0,25,200,66],[0,25,200,22],[0,25,300,66],[0,25,300,22],[0,50,100,66],[0,50,100,22],[0,50,200,66],[0,50,200,22],[0,50,300,66],[0,50,300,22],[0,80,100,66],[0,80,100,22],[0,80,200,66],[0,80,200,22],[0,80,300,66],[0,80,300,22],[12,25,100,66],[12,25,100,22],[12,25,200,66],[12,25,200,22],[12,25,300,66],[12,25,300,22],[12,50,100,66],[12,50,100,22],[12,50,200,66],[12,50,200,22],[12,50,300,66],[12,50,300,22],[12,80,100,66],[12,80,100,22],[12,80,200,66],[12,80,200,22],[12,80,300,66],[12,80,300,22]]')
		= vals
]
